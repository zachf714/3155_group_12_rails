<h1>New Activity</h1>
<%= form_for :user, url: new_preload_path do |f| %>
    <p>
        <h2>Activity Zip Code</h2>
        <%= f.text_field :zip %>
    </p>
    
    <p>
        <h2>Activity Title</h2>
        <%= f.text_field :title %>
    </p>
    
    <p>
        <h2>Activity Description</h2>
        <%= f.text_area :body %>
    </p>
    
    <p>
        <%= f.submit "Save Activity"%>
    </p>
    
<% end %>

<%= link_to 'Back', controller: 'preload' %>